<?xml version="1.0" encoding="UTF-8"?>
<main>
    <inputs>
        <password>
            <minLength>8</minLength>
        </password>

        <email>
            <regex>/[a-zA-Z0-9_\-.+]+@[a-zA-Z0-9-]+.[a-zA-Z]+/</regex>
        </email>

        <titleTask>
            <maxLength>25</maxLength>
        </titleTask>

        <contentTask>
            <maxLength>145</maxLength>
        </contentTask>

        <endDateTask>
            <dateFormat>Y-m-d</dateFormat>
        </endDateTask>

        <titleTodo>
            <maxLength>10</maxLength>
        </titleTodo>

        <descriptionTodo>
            <maxLength>200</maxLength>
        </descriptionTodo>

        <inputText>
            <regex></regex>
        </inputText>
    </inputs>
</main>